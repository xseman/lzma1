{
	"name": "lzma1",
	"version": "0.0.1",
	"license": "Apache-2.0",
	"author": "Filip Seman <filip.seman@pm.me>",
	"description": "A JavaScript implementation of the Lempel-Ziv-Markov (LZMA) chain compression algorithm",
	"funding": "https://github.com/sponsors/xseman",
	"homepage": "https://github.com/xseman/lzma1#readme",
	"keywords": [
		"lzma",
		"lzma1",
		"compression",
		"decompression"
	],
	"repository": {
		"type": "git",
		"url": "git+https://github.com/xseman/lzma1.git"
	},
	"scripts": {
		"build": "tsc",
		"test": "node --test --test-reporter=spec --loader=tsx ./*.test.ts",
		"test:watch": "node --test --watch --test-reporter=spec --loader=tsx ./*.test.ts",
		"typecheck": "tsc --noEmit",
		"version": "git flow release start v$npm_package_version"
	},
	"devDependencies": {
		"@types/node": ">=16.17",
		"dprint": "~0.35.0",
		"tsx": "~3.12.0",
		"typescript": "~5.0.0"
	},
	"type": "module",
	"types": "lib/lzma.d.ts",
	"exports": {
		"types": "./lib/lzma.d.ts",
		"default": "./lib/lzma.js"
	},
	"files": [
		"lib"
	],
	"engines": {
		"node": ">=14.16",
		"npm": ">=7.0"
	}
