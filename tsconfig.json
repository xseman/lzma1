{
	"compilerOptions": {
		"target": "ES2020",
		"module": "Node16",
		"moduleResolution": "Node16",
		"outDir": "./lib",
		"downlevelIteration": true,
		"strict": true,
		"declaration": true,
		"noImplicitAny": true,
		"noImplicitThis": true,
		"noImplicitReturns": true,
		"esModuleInterop": true,
		"resolveJsonModule": true,
		"useUnknownInCatchVariables": false
	},
	"include": [
		"./lzma.ts"
	],
	"exclude": [
		"node_modules"
	]
}
